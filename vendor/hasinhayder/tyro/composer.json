{
    "name": "hasinhayder/tyro",
    "description": "Tyro - The ultimate Authentication, Authorization, and Role & Privilege Management solution for Laravel 12",
    "keywords": [
        "laravel",
        "authentication",
        "authorization",
        "roles",
        "privileges",
        "rbac",
        "permissions",
        "sanctum",
        "api",
        "acl"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0"
    },
    "require-dev": {
        "laravel/pint": "^1.25",
        "nunomaduro/collision": "^8.6",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "HasinHayder\\Tyro\\": "src/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HasinHayder\\Tyro\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "HasinHayder\\Tyro\\Providers\\TyroServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
